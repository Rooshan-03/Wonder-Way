    "expo-status-bar": "~3.0.9",
    "lottie-react-native": "~7.3.1",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-app-intro-slider": "^4.0.4",
